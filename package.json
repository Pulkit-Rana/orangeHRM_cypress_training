{
  "scripts": {
    "clean:reportsfolder": "rmdir /s /q cypress\\reports",
    "add:reportfolder": "mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reportsfolder && npm run add:reportfolder",
    "start": "npx cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run start || npm run posttest",
    "cy:parallel": "cypress-parallel -s start -t 2 -d cypress/e2e/*.js",
    "parallel": "npm run cy:parallel && npm run posttest"
  },
  "devDependencies": {
    "cypress": "^13.3.2",
    "cypress-multi-reporters": "^1.6.4",
    "mocha": "^8.4.0",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "dependencies": {
    "cypress-parallel": "^0.13.0"
  }
}
